{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "semicolon": true,
        "member-access": true,
        "max-classes-per-file": false,
        "no-empty": false,
        "no-empty-interface": false,
        "variable-name": false,
        "no-shadowed-variable": false,
        "forin": false,
        "ban-types": false,
        "array-type": false
        
    },
    "rulesDirectory": []
}